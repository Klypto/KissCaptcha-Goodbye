{
  "background": {
    "persistent": false,
    "scripts": [ "js/background.js" ]
  },
  "author": "Klypto",
  "browser_action": {
    "default_icon": "icon.png"
  },
  "content_scripts": [

    {
      "all_frames": true,
      "js": [ "js/jquery/jquery.js", "js/AreYouHuman.js", "js/js/md5.js" ],
      "matches": [ "*://*.kissanime.ru/Special/AreYouHuman2*" ],
      "run_at": "document_start"
    }
  ],
  "description": "Quickly answers most of those annoying CAPTCHA questions on Kissanime.",
  "icons": {
    "128": "icon.png"
  },
  "manifest_version": 2,
  "minimum_chrome_version": "45.0.2454",
  "name": "KissCaptcha Goodbye",
  "permissions": [ "activeTab" ],
  "update_url": "https://clients2.google.com/service/update2/crx",
  "version": "0.9.0",
  "version_name": "0.9.0",
  "web_accessible_resources": [ "js/*" ]
}
